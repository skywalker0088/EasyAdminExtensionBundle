{% set default_filters = guess_default_filters(
  field_options.template_options.object_fqcn, field_options.template_options.parent_object_property, item
  ) %}
{% set filters = default_filters|merge(field_options.template_options.filters) %}

{% set tpl_params = {
  filters: filters,
  sort: field_options.template_options.sort
} %}
{% if 'document' == field_options.template_options.object_type %}
  {% set tpl_params = tpl_params|merge({document: field_options.template_options.document}) %}
{% else %}
  {% set tpl_params = tpl_params|merge({entity: field_options.template_options.entity}) %}
{% endif %}

{% include '@EasyAdmin/includes/_include_embedded_list.html.twig' with tpl_params only %}
